package com.practice;

public class StringDemo {
	public static void main(String[] args) {
		try {
			String str = null;
			System.out.println(str.charAt(0));
		}catch(Exception e) {
			System.out.println("Null Pointer exception");
			System.exit(0);
		}finally {
			System.out.println("Hello world, I am Back..!!");
		}
	}
}
